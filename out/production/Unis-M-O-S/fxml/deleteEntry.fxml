<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.DialogWindow">
   <children>
      <AnchorPane fx:id="titlePane" prefHeight="34.0" prefWidth="400.0" style="-fx-background-color: #2ecc71;">
         <children>
            <Label alignment="CENTER" layoutX="-5.0" layoutY="-1.0" prefHeight="50.0" prefWidth="406.0" stylesheets="@../css/studentFrom.css" text="Delete" textFill="WHITE">
