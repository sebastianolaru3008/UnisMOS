<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.DialogWindow">
   <children>
      <AnchorPane prefHeight="34.0" prefWidth="400.0" style="-fx-background-color: #2ecc71;">
         <children>
            <Label alignment="CENTER" layoutX="-5.0" layoutY="-1.0" prefHeight="50.0" prefWidth="406.0" stylesheets="@../css/studentFrom.css" text="Assign Tax" textFill="WHITE">
               <font>
                  <Font name="Montserrat ExtraBold Italic" size="29.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="cancelButton" layoutX="32.0" layoutY="143.0" mnemonicParsing="false" onAction="#goBackAction" prefHeight="34.0" prefWidth="77.0" text="Cancel" />
      <Button fx:id="addTaxButton" layoutX="290.0" layoutY="143.0" mnemonicParsing="false" onAction="#assignTaxAction" prefHeight="34.0" prefWidth="77.0" text="Asign Tax" />
      <ComboBox fx:id="studentComboBox" layoutX="34.0" layoutY="88.0" prefWidth="150.0" promptText="Student" />
      <ComboBox fx:id="taxComboBox" layoutX="217.0" layoutY="88.0" prefWidth="150.0" promptText="Tax" />
   </children>
   <effect>
      <DropShadow />
   </effect>
</AnchorPane>
